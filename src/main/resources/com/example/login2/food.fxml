<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #160A34;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login2.foodcontroller">
    <children>
        <CheckBox fx:id="breakfast_checkbox" layoutX="109.0" layoutY="157.0" mnemonicParsing="false" onMouseClicked="#breakfast_checkbox_marked" prefHeight="40.0" prefWidth="222.0" text="breakfast" textFill="#cf0b3c">
            <font>
                <Font name="Arial Rounded MT Bold" size="28.0" />
            </font>
        </CheckBox>
        <CheckBox fx:id="lunch_checkbox" layoutX="109.0" layoutY="225.0" mnemonicParsing="false" onMouseClicked="#lunch_checkbox_marked" prefHeight="40.0" prefWidth="222.0" text="lunch" textFill="#cf0b3c">
            <font>
                <Font name="Arial Rounded MT Bold" size="28.0" />
            </font>
        </CheckBox>
        <CheckBox fx:id="dinner_checkbox" layoutX="109.0" layoutY="291.0" mnemonicParsing="false" onMouseClicked="#dinner_checkbox_marked" prefHeight="40.0" prefWidth="222.0" text="dinner" textFill="#cf0b3c">
            <font>
                <Font name="Arial Rounded MT Bold" size="28.0" />
            </font>
        </CheckBox>
        <Button fx:id="food_button" layoutX="425.0" layoutY="319.0" mnemonicParsing="false" onAction="#food_button_clicked" prefHeight="62.0" prefWidth="139.0" style="-fx-background-color: #cf0b3c;" text="Done" textFill="WHITE">
            <font>
                <Font name="System Bold" size="27.0" />
            </font>
        </Button>
      <Label layoutX="109.0" layoutY="95.0" text="room number" textFill="#cf0b3c">
         <font>
            <Font name="Arial Rounded MT Bold" size="28.0" />
         </font>
      </Label>
      <TextField fx:id="roomIDFld" layoutX="318.0" layoutY="99.0" prefHeight="34.0" prefWidth="150.0" promptText="Room ID" />
    </children>
</AnchorPane>
